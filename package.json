{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "Easy to use, mern boilerplate. Ready to deploy to heroku, no further configuration required.",
  "main": "index.js",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "dev:app": "parcel watch src/app/index.html",
    "dev:server": "nodemon --watch src/server src/server/index.js",
    "dev": "concurrently -n app,server -c green,blue \"npm run dev:app\" \"npm run dev:server\"",
    "build": "parcel build src/app/index.html --out-dir src/server/public/app --public-url /app",
    "start": "NODE_ENV=production node src/server/index.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "Lukas Gisder-Dub√©",
  "license": "ISC",
  "dependencies": {
    "@progress/kendo-react-dropdowns": "^2.0.0",
    "@progress/kendo-react-intl": "^2.0.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "chalk": "^2.4.1",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "helmet": "^3.13.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "mdbreact": "^4.8.0",
    "moment": "^2.22.2",
    "mongoose": "^5.7.3",
    "morgan": "^1.9.1",
    "popper.js": "^1.14.4",
    "react": "^16.5.1",
    "react-360": "^1.0.0",
    "react-dom": "^16.5.2",
    "react-native": "^0.57.1",
    "react-native-sound": "^0.10.9",
    "react-reveal": "^1.2.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-super-select": "^1.0.19",
    "reactstrap": "^6.4.0",
    "semantic-ui-react": "^0.82.5",
    "spotify-web-api-js": "^1.1.1",
    "spotify-web-api-node": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^4.0.1",
    "cssnano": "^4.1.0",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.9.7",
    "react-hot-loader": "^4.3.11"
  }
}
